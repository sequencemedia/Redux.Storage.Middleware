{
  "name": "redux-storage-middleware",
  "version": "0.0.4",
  "description": "Redux Storage middleware",
  "main": "./index.js",
  "scripts": {
    "prepublishOnly": "npm run babel",
    "build": "npm run babel",
    "build:clean": "rm -rf lib; npm run babel",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint src config/mocha",
    "lint:fix": "npm run lint -- --fix",
    "test": "nyc mocha config/mocha --require babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sequencemedia/Redux.Storage.Middleware.git"
  },
  "license": "MIT",
  "author": "Jonathan Perry for Sequence Media Limited",
  "engines": {
    "node": ">=7.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "html"
    ],
    "report-dir": "config/coverage"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.10.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.3.0",
    "sinon": "^4.0.2",
    "sinon-chai": "^2.14.0"
  }
}
