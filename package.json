{
  "name": "redux-storage-middleware",
  "version": "0.0.8",
  "description": "Redux Storage middleware",
  "main": "./index.js",
  "scripts": {
    "prepublishOnly": "npm run clean; npm run babel",
    "clean": "rm -rf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint src config/mocha",
    "lint:fix": "npm run lint -- --fix",
    "test": "nyc mocha config/mocha --require babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sequencemedia/Redux.Storage.Middleware.git"
  },
  "license": "MIT",
  "author": "Jonathan Perry for Sequence Media Limited",
  "engines": {
    "node": ">=8.11.3"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "html"
    ],
    "report-dir": "config/coverage"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^1.0.0-rc.9",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix; npm run clean; npm run babel; git add ."
    }
  }
}
