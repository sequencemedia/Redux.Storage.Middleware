{
  "compact": true,
  "comments": false,
  "presets": [
    [
      "env", {
        "targets": {
          "node": "8.11.3",
          "browsers": [
            "last 2 versions",
            "safari >= 7"
          ]
        },
        "useBuiltIns": true
      }
    ],
    "stage-0"
  ],
  "plugins": [
    [
      "module-resolver", {
        "alias": {
          "redux-storage-middleware": "./src"
        }
      }
    ]
  ]
}
